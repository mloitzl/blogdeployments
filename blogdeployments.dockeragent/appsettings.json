{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Containers": [
    {
      "image": "couchdb",
      "tag": "3.2.2",
      "name": "started_from_netcore",
      "Mounts": [
        {
          "source": "$HOME/.secrets/couchdb/local.ini",
          "target": "/opt/couchdb/etc/local.ini"
        }
      ],
      "Ports": [
        {
          "hostbindings": [
            {
              "HostPort": 5985
            }
          ],
          "container": 5984
        }
      ]
    }
  ]
}
